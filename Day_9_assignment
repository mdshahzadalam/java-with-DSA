for(i=1;i<=n;i++)
{
 for(j=1;j<=n;j++)
 {
     if(i<=n && j==1  || i==n && j<=n)
     {
         System.out.print("*");
     }
     else{
         System.out.print(" ");
     }
 }
 System.out.println();
}

System.out.print("   ");  

System.out.println();

