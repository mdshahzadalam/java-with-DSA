{
 for(j=1;j<=n;j++)
 {
     if(i<=n && j==1 || i==1 && j<=n || i<=n && j==n || i==n/2 && j<=n)
     {
         System.out.print("*");
     }
     else{
         System.out.print(" ");
     }
 }
 System.out.println();
}

System.out.print("   ");  

System.out.println();

//M
